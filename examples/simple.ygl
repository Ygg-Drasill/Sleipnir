node Foo {
    out {
        int x = 1;
        int y = 2;
    }
}

node Bar {
    in {
        int a;
        int b;
    }

    out {
        int c;
    }

    process {
        int j = 2 + 2;
        out.c = in.a;
        out.c = out.c + j;
    }
}

node Add0 : Add;

node Print0 : Print;

Foo.out.x -> Bar.in.a;
Foo.out.y -> Bar.in.b;
Foo.out.x -> Add0.in.a;
Bar.out.c -> Add0.in.b;
Add0.out.result -> Print0.in.text;